UIGradient.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0.00, 0.60), NumberSequenceKeypoint.new(0.60, 0.89), NumberSequenceKeypoint.new(0.75, 1.00), NumberSequenceKeypoint.new(1.00, 1.00), NumberSequenceKeypoint.new(1.00, 0.60)}
UIGradient.Parent = Background

Main.Name = "Main"
